{
  "name": "altrp-static",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "next dev -p 3105 -H 0.0.0.0 --webpack",
    "dev:wrangler": "bun run build:static && wrangler pages dev --port 3305",
    "dev:all": "cross-env NEXT_PORT=3103 WRANGLER_PORT=3303 PROXY_PORT=3405 node scripts/dev-server.mjs",
    "build": "next build --experimental-build-mode compile",
    "build:static": "cross-env STATIC_EXPORT=true next build",
    "start": "next start -p 3100",
    "type-check": "tsc --noEmit",
    "type-check:save": "powershell -Command \"if (-not (Test-Path test-results)) { New-Item -ItemType Directory -Path test-results | Out-Null }; tsc --noEmit *> test-results/errors.log 2>&1\"",
    "d1:create": "wrangler d1 create altrp-site-db",
    "d1:migrate:local": "wrangler d1 execute altrp-site-db --local --file=../../migrations/site/app-schema.sql",
    "d1:migrate:remote": "wrangler d1 execute altrp-site-db --remote --file=../../migrations/site/app-schema.sql",
    "d1:migrate:users:local": "wrangler d1 execute altrp-site-db --local --file=../../migrations/site/users-table.sql",
    "d1:migrate:users:remote": "wrangler d1 execute altrp-site-db --remote --file=../../migrations/site/users-table.sql",
    "d1:migrate:taxonomy-uuid:local": "wrangler d1 execute altrp-site-db --local --file=../../migrations/site/20250115_add_uuid_to_taxonomy.sql",
    "d1:migrate:taxonomy-uuid:remote": "wrangler d1 execute altrp-site-db --remote --file=../../migrations/site/20250115_add_uuid_to_taxonomy.sql",
    "d1:migrate:taxonomy-title-json:local": "wrangler d1 execute altrp-site-db --local --file=../../migrations/site/20250115_convert_taxonomy_title_to_json.sql",
    "d1:migrate:taxonomy-title-json:remote": "wrangler d1 execute altrp-site-db --remote --file=../../migrations/site/20250115_convert_taxonomy_title_to_json.sql",
    "d1:query:local": "wrangler d1 execute altrp-site-db --local",
    "d1:query:remote": "wrangler d1 execute altrp-site-db --remote",
    "hash-password": "node scripts/hash-password.mjs",
    "test:int": "bun test --config ./tsconfig.json test/int/"
  },
